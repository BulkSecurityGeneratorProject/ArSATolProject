    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity MethodeLutte.
    -->
    <changeSet id="20190216191706-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ravageur_id"
                                 baseTableName="methode_lutte"
