<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Insecte.
    -->
    <changeSet id="20190216191704-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="methode_id"
                                 baseTableName="insecte"
                                 constraintName="fk_insecte_methode_id"
                                 referencedColumnNames="id"
                                 referencedTableName="methode_lutte"/>

        <addForeignKeyConstraint baseColumnNames="chercheur_id"
                                 baseTableName="insecte"
                                 constraintName="fk_insecte_chercheur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="chercheur"/>

        <addForeignKeyConstraint baseColumnNames="administrateur_id"
                                 baseTableName="insecte"
                                 constraintName="fk_insecte_administrateur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="administrateur"/>

    </changeSet>
</databaseChangeLog>
