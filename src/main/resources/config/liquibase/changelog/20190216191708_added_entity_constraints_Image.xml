<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Image.
    -->
    <changeSet id="20190216191708-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="insecte_id"
                                 baseTableName="image"
                                 constraintName="fk_image_insecte_id"
                                 referencedColumnNames="id"
                                 referencedTableName="insecte"/>

        <addForeignKeyConstraint baseColumnNames="attaque_id"
                                 baseTableName="image"
                                 constraintName="fk_image_attaque_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attaque"/>

        <addForeignKeyConstraint baseColumnNames="agriculteur_id"
                                 baseTableName="image"
                                 constraintName="fk_image_agriculteur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agriculteur"/>

    </changeSet>
</databaseChangeLog>
